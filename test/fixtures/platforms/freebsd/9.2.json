{
    "apache": {
        "version": "2.2",
        "binary": "/usr/local/sbin/httpd",
        "package": "apache22",
        "dir": "/usr/local/etc/apache22",
        "lib_dir": "/usr/local/libexec/apache22",
        "libexec_dir": "/usr/local/libexec/apache22",
        "cache_dir": "/var/cache/apache22",
        "conf": "/usr/local/etc/apache22/httpd.conf",
        "perl_pkg": "perl5",
        "log_dir": "/var/log",
        "root_group": "wheel",
        "service_name": "apache22",
        "default_site_name": "default",
        "default_site_enabled": false,
        "default_modules": [
            "status",
            "alias",
            "auth_basic",
            "authn_file",
            "authz_core",
            "authz_groupfile",
            "authz_host",
            "authz_user",
            "autoindex",
            "dir",
            "env",
            "mime",
            "negotiation",
            "setenvif"
        ]
    }
}
